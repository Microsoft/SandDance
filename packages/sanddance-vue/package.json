{
  "name": "@msrvida/sanddance-vue",
  "version": "3.0.0-beta.1",
  "description": "SandDance visualization canvas Vue component",
  "main": "dist/SandDanceVue.umd.js",
  "module": "dist/SandDanceVue.esm.js",
  "unpkg": "dist/SandDanceVue.min.js",
  "browser": {
    "./sfc": "src/SandDanceVue.vue"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/Letter.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/Letter.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/Letter.min.js"
  },
  "devDependencies": {
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1"
  },
  "dependencies": {
    "@msrvida/sanddance": "3.0.0-beta.1",
    "just-compare": "^1.3.0"
  },
  "author": "",
  "license": "MIT",
  "keywords": []
}
