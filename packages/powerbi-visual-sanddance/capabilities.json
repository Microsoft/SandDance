{
    "dataRoles": [
        {
            "displayName": "Unique Id",
            "name": "uid",
            "kind": "GroupingOrMeasure"
        },
        {
            "displayName": "X Axis",
            "name": "x",
            "kind": "GroupingOrMeasure"
        },
        {
            "displayName": "Y Axis",
            "name": "y",
            "kind": "GroupingOrMeasure"
        },
        {
            "displayName": "Z Axis",
            "name": "z",
            "kind": "GroupingOrMeasure"
        },
        {
            "displayName": "Color by",
            "name": "color",
            "kind": "GroupingOrMeasure"
        },
        {
            "displayName": "Sort by",
            "name": "sort",
            "kind": "GroupingOrMeasure"
        }
    ],
    "objects": {
        "layout": {
            "displayName": "Layout",
            "properties": {
                "charttype": {
                    "displayName": "Chart type",
                    "type": {
                        "enumeration": [
                            {
                                "value": "barchart",
                                "displayName": "Column"
                            },
                            {
                                "value": "scatterplot",
                                "displayName": "Scatter"
                            }
                        ]
                    }
                },
                "showaxes": {
                    "displayName": "Show axes",
                    "type": {
                        "bool": true
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "uid": {
                        "max": 1
                    },
                    "x": {
                        "max": 1
                    },
                    "y": {
                        "max": 1
                    },
                    "z": {
                        "max": 1
                    },
                    "color": {
                        "max": 1
                    },
                    "sort": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "uid"
                    },
                    "dataReductionAlgorithm": {
                        "sample": {
                            "count": 30000
                        }
                    }
                },
                "values": {
                    "group": {
                        "by": "grouping",
                        "select": [
                            {
                                "bind": {
                                    "to": "x"
                                }
                            },
                            {
                                "bind": {
                                    "to": "y"
                                }
                            },
                            {
                                "bind": {
                                    "to": "z"
                                }
                            },
                            {
                                "bind": {
                                    "to": "color"
                                }
                            },
                            {
                                "bind": {
                                    "to": "sort"
                                }
                            }
                        ]
                    }
                }
            }
        }
    ]
}